{{#if (has-block)}}
  {{yield properties filterRowObject.property }}
{{else}}
  {{#power-select
      selected=filterRowObject.property
      options=properties
      onchange=(action "updateSelectedProperty" (action (mut filterRowObject.property)) filterRowObject)
      tagName="div"
      searchField="label"
      class="property-selector"
      as |filter|
  }}
    {{get filter "label"}}
  {{/power-select}}
{{/if}}
