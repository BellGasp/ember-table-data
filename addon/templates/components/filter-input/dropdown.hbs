{{#power-select
    options=options
    tagName="div"
    class="dropdown"
    selected=selectedValue
    matcher=(action "customMatcher")
    onChange=(action "valueChanged")
    as |option|
}}
  {{#if (get option labelPath)}}
    {{get option labelPath}}
  {{else}}
    {{option}}
  {{/if}}
{{/power-select}}
